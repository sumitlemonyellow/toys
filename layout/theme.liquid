                    </div>
                  </div>
                  <div class="sort-right">


                    {% assign sort_view = collection.sort_view | default: collection.default_sort_view %}
                    <div class="btn-group" role="group" aria-label="Basic radio toggle button group"
                      aria-describedby="a11y-refresh-page-message a11y-selection-message">

                      <input type="radio" class="btn-check" name="sort_view" id="btnradio1" value="block"
                        autocomplete="off" {% if param_page == "block" %} checked {% endif %}>
                      <label class="btn btn-chck SortView" sortv="btnradio1" for="btnradio1">Block</label>

                      <input type="radio" class="btn-check" name="sort_view" id="btnradio2" value="list"
