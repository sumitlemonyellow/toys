                      <label class="btn btn-chck SortView" sortv="btnradio2" for="btnradio2">List</label>

                    </div>

                  </div>
                </div>
              </div>
            </div>
            <div class="products py-5 ">




              {{ content_for_layout }}







            </div>
          </div>
        </div>
      </div>
    </div>


  </div>
  {{ 'theme.js' | asset_url | script_tag }}
