                    {{ filter_value.label }} X
                  </a>
                  {%- endfor -%}
                  {%- endif- %}
                  {%- endfor -%}
                </div>
              </form>

            </div>

          </div>
        </div>
        <div class="col-9">
          <div class="products-wrapper ">
            <div class="row">
              <div class="col-12">
                <div class="sort-row sort-filters d-flex justify-content-between">
                  <div class="sort-left">
                    <div class="sort-filter-dropdown-wrapper">


                      <script>
                        window.theme = window.theme || {};
                        // Override default values of shop.strings for each template.
